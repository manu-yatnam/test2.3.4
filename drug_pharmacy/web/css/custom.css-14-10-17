@media only screen and (max-width: 768px){
	#quote-form,
	#productqa_form .fieldset,
	#productqa_form .actions-toolbar,
	#faq_form {
		width: 100%;
		margin-top: 25px;
	}
}
@media only screen and (max-width: 639px) {
	#quote-form fieldset label.label,
	#productqa_form fieldset label.label,
	#faq_form fieldset label.label {
		width: 100%;
		padding-right: 0;
		text-align: left;
	}
	#quote-form fieldset .field,
	#productqa_form fieldset .field,
	#faq_form fieldset .field {
		margin-bottom: 8px;
	}
	#quote-form fieldset .control,
	#productqa_form fieldset .control,
	#faq_form fieldset .control {
		width: 100%;
		padding-left: 0px;
	}
	#quote-form .actions-toolbar .primary,
	#quote-form .actions-toolbar .primary button,
	#productqa_form .actions-toolbar .primary,
	#productqa_form .actions-toolbar .primary button.action {
		width: 100%;
	}
}


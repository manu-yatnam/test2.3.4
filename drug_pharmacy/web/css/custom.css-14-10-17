/*======== Faq ========*/ 
#accordion > li > div {
	background-color: #7EC7E1;
	font-weight: 600;
	position: relative;
	padding: 8px 10px 8px 10px;
}
#accordion > li > div:after {
	content: "\f107";
	display: inline-block;
    font-family: FontAwesome;
    font-style: normal;
    font-weight: normal;
    line-height: 1;
    -webkit-font-smoothing: antialiased;
    font-size: 18px;
    float: right;
    backface-visibility: hidden;
    transition: all 500ms ease; 
}
#accordion ul {
	padding: 10px;
}
#accordion ul li {
	padding-left: 0;
	font-family: inherit;
	margin: 0 10px 10px 10px;
	font-family: inherit;
}
#accordion ul li.answerfaq {
	overflow: hidden; 
}
#accordion ul li .question-ans {
	font-family: inherit;s
	float: left;
    padding: 0 0 0 50px;
    position: relative;
    font-weight: 500;
    font-style: normal;
    color: #5d5d5d;
    cursor: default;
}
#accordion ul li.answerfaq .question-ans {
	font-weight: 400;
}
#accordion ul li .question-ans:before {
	position: absolute;
	left: 0;
	content: '';
	font-weight: 500;
	color: #5d5d5d;
}
#accordion ul li.questionfaq .question-ans:before {
	content: 'Qns : '
}
#accordion ul li.answerfaq .question-ans:before {
	content: 'Ans : '
}
#accordion ul li.answerfaq > p {
	display: inline-block;
	font-family: inherit;
	float: left;
    width: calc(100% - 50px);
    padding-top: 8px;
}
.quote-heading {
	font-weight: 500;
}
.faqform h1 {
	border-bottom: 1px solid #dbdbdb;
